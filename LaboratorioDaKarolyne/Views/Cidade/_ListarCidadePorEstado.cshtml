@model IEnumerable<LaboratorioDaKarolyne.Models.Cidade>
<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.Nome)</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(i => item.Nome)</td>
                <td><a href="#" id="@item.IdCidade" data-nome="@item.Nome">Selecionar <i class="glyphicon glyphicon-check"></i></a></td>               
            </tr>
        }
    </tbody>
</table>


