@model LaboratorioDaKarolyne.Models.Cidade
@{
    ViewBag.Title = "Nova Cidade";
}

<h2>Nova Cidade</h2>
<hr />

<form method="post">
    <div class="form-group">
        @Html.LabelFor(model=>model.Nome)
        @Html.EditorFor(model=>model.Nome, new { htmlAttributes = new { @class="form-control"}})
    </div>
    <div class="form-group">
        @Html.LabelFor(model=>model.EnumEstado)
            @*@Html.DropDownListFor(model => model.EnumEstado)*@
         @Html.EditorFor(model=>model.EnumEstado, new { htmlAttributes = new {@class="form-control"}})
    </div>
    <div class="form-group">
        <div class="col-md-offset-1">
            <input type="submit" value="Salvar" class="btn btn-success" />
            <input type="submit" value="Cancelar" class="btn btn-primary" />
        </div>
    </div>
</form>





